{{> default_header}}

<section class="section-top highlight">
    <div class="container">
        <div class="row section-header">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">Sitemap</h2>
            </div>
        </div>
    </div>
</section>
<section>
    <div class="container">
        <ul>
          {{#each _base.menu}}
              <li class="{{class}} {{equal @key @root._page 'active'}}">
                  <a href="{{@key}}.html">{{title}}</a>
              </li>
          {{/each}}
        </ul>
    </div>
</section>

{{> default_footer}}
